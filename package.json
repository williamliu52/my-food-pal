{
  "name": "my-food-pal",
  "version": "1.0.0",
  "description": "Food logger",
  "engines": {
    "node": "6.11.x"
  },
  "scripts": {
    "server": "node server",
    "start": "concurrently \"node server\" \"cd client && npm start\"",
    "debug": "concurrently \"node --inspect server\" \"cd client && npm start\"",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.14.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "request": "^2.81.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamliu52/my-food-pal.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
